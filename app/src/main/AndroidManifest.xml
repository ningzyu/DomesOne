<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nzy.dome1">
    <!--创建与删除权限-->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <!--写入权限-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.Activity0" />
        <activity android:name=".ui.Activity01" />
        <activity android:name=".ui.Activity02" />
        <activity android:name=".ui.Activity03" />
        <activity android:name=".ui.Activity04" />
        <activity android:name=".ui.Activity05" />
        <activity android:name=".ui.Activity06" />
        <activity android:name=".ui.Activity07" />
        <activity android:name=".ui.Activity08" />
        <activity android:name=".ui.Activity09" />
        <activity android:name=".ui.Activity10"></activity>
    </application>

</manifest>